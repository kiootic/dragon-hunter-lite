{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es2015",
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "baseUrl": "../..",
    "paths": {
      "app": [
        "src/app"
      ],
      "app/*": [
        "src/app/*"
      ],
      "assets/*": [
        "assets/*"
      ]
    }
  }
}