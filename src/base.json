{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "..",
    "lib": [],
    "paths": {
      "app": [
        "src/app"
      ],
      "app/*": [
        "src/app/*"
      ],
      "worker": [
        "src/worker"
      ],
      "worker/*": [
        "src/worker/*"
      ],
      "common/*": [
        "src/common/*"
      ],
      "data/*": [
        "src/data/*"
      ],
      "assets/*": [
        "assets/*"
      ]
    }
  }
}